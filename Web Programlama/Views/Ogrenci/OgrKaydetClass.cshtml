@model Ogrenci

<h2 style="color: green">Ogr Kaydedildi</h2>

<table width="70%" class=" table table-active ">

	<tr>
		<td>
			Öğrenci Ad
		</td>
		<td>:</td>
		<td>
			@Model.OgrAd
		</td>
	</tr>


	<tr>
		<td>
			Öğrenci Soyad
		</td>
		<td>:</td>
		<td>
			@Model.OgrSoyad
		</td>
	</tr>



	<tr>
		<td>
			Öğrenci No
		</td>
		<td>:</td>
		<td>
			@Model.OgrNo
		</td>
	</tr>


	

</table>